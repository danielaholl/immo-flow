{
  "expo": {
    "name": "ImmoFlow",
    "slug": "immoflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF385C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.immoflow.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF385C"
      },
      "package": "com.immoflow.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "immoflow",
    "plugins": [
      "expo-router"
    ]
  }
}
